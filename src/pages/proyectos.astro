---
import { Base } from "@/components/Base";
import MenuNav from "@/components/cabecita/MenuNav";
import { SelectProyecto } from "@/components/cabecita/SelectProyecto";

import MainLayout from "@/layout/MainLayout.astro";

const ruta = Astro.url.pathname;



---


<MainLayout>
  <section class="bg-[url('/fondo_perritos.png')] h-fit bg-cover bg-no-repeat bg-center">
    <Base client:idle/>
    <MenuNav client:idle ruta={ruta} />

  <SelectProyecto client:idle />
  
</section>
</MainLayout>
